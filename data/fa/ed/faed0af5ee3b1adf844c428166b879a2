            "key": "PACB6QYR",
            "version": 83846,
            "itemType": "journalArticle",
            "title": "Effect of fertility control on a population's productivity",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "G.",
                    "lastName": "Caughley"
                },
                {
                    "creatorType": "author",
