        "data": {
            "key": "ID9AH3QB",
            "version": 85357,
            "itemType": "journalArticle",
            "title": "Citizen Science and Biological Invasions: A Review",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "João",
                    "lastName": "Encarnação"
                },
                {
                    "creatorType": "author",
                    "firstName": "Maria Alexandra",
                    "lastName": "Teodósio"
                },
                {
                    "creatorType": "author",
                    "firstName": "Pedro",
