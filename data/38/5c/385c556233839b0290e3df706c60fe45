            "version": 87400,
            "itemType": "journalArticle",
            "title": "Predicting global ascidian invasions",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Daniel M.",
                    "lastName": "Lins"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paulo",
                    "lastName": "de Marco"
