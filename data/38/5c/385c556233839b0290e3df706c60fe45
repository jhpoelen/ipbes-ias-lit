            "itemType": "journalArticle",
            "title": "Spatial pattern and determinants of global invasion risk of an invasive species, sharpbelly <i>Hemiculter leucisculus</i> (Basilesky, 1855)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Xianghong",
                    "lastName": "Dong"
                },
                {
                    "creatorType": "author",
                    "firstName": "Tao",
                    "lastName": "Ju"
