            "key": "4AKDGTTI",
            "version": 84068,
            "itemType": "journalArticle",
            "title": "Are urban systems beneficial, detrimental, or indifferent for biological invasion?",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Marc W.",
                    "lastName": "Cadotte"
                },
                {
                    "creatorType": "author",
                    "firstName": "Simone Louise E.",
                    "lastName": "Yasui"
                },
                {
                    "creatorType": "author",
                    "firstName": "Stuart",
