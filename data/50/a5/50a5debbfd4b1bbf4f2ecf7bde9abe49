            "version": 84067,
            "itemType": "journalArticle",
            "title": "Multiple effects of urbanization on the biodiversity of developing countries: The case of a fast-growing metropolitan area (Concepción, Chile)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Aníbal",
                    "lastName": "Pauchard"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mauricio",
                    "lastName": "Aguayo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Eduardo",
                    "lastName": "Peña"
                },
