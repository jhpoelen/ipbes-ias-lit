                {
                    "creatorType": "author",
                    "firstName": "Lisa",
                    "lastName": "Matthias"
                },
                {
                    "creatorType": "author",
                    "firstName": "Bree",
                    "lastName": "Norlander"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ashley",
