            "key": "IZLPFAMD",
            "version": 83908,
            "itemType": "journalArticle",
            "title": "Adaptive Comanagement: a Systematic Review and Analysis",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ryan",
                    "lastName": "Plummer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Beatrice",
                    "lastName": "Crona"
                },
                {
                    "creatorType": "author",
                    "firstName": "Derek R.",
                    "lastName": "Armitage"
                },
                {
                    "creatorType": "author",
                    "firstName": "Per",
                    "lastName": "Olsson"
                },
                {
                    "creatorType": "author",
                    "firstName": "Maria",
