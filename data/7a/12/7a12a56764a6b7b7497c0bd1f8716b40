            "key": "ZAF6GP67",
            "version": 83887,
            "itemType": "journalArticle",
            "title": "Second generation biofuels and bioinvasions: An evaluation of invasive risks and policy responses in the United States and Canada",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "A.L.",
                    "lastName": "Smith"
                },
                {
                    "creatorType": "author",
                    "firstName": "N.",
                    "lastName": "Klenk"
                },
                {
                    "creatorType": "author",
                    "firstName": "S.",
                    "lastName": "Wood"
                },
                {
                    "creatorType": "author",
                    "firstName": "N.",
