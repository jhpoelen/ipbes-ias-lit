            "version": 83832,
            "itemType": "journalArticle",
            "title": "Ecological Effects of Invasive Arthropod Generalist Predators",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "William E.",
                    "lastName": "Snyder"
                },
                {
                    "creatorType": "author",
                    "firstName": "Edward W.",
                    "lastName": "Evans"
                }
