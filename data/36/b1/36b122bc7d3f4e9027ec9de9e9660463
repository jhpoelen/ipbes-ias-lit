            "key": "RHQZVGY3",
            "version": 87276,
            "itemType": "journalArticle",
            "title": "Optimal dynamic control of invasions: Applying a systematic conservation approach",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "V. M.",
                    "lastName": "Adams"
                },
                {
                    "creatorType": "author",
                    "firstName": "S.A.",
                    "lastName": "Setterfield"
                }
