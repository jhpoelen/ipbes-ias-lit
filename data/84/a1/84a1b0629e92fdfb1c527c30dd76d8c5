            "version": 87406,
            "itemType": "journalArticle",
            "title": "What's wrong with novel ecosystems, really?",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "James R.",
                    "lastName": "Miller"
                },
                {
                    "creatorType": "author",
                    "firstName": "Brandon T.",
                    "lastName": "Bestelmeyer"
                }
