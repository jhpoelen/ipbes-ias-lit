            "version": 83720,
            "itemType": "journalArticle",
            "title": "Space, time and aliens: charting the dynamic structure of Galápagos pollination networks",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Anna",
                    "lastName": "Traveset"
                },
                {
                    "creatorType": "author",
                    "firstName": "Susana",
                    "lastName": "Chamorro"
                },
                {
                    "creatorType": "author",
