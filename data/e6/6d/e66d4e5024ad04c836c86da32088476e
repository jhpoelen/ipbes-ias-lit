            "key": "QMSATVS6",
            "version": 83925,
            "itemType": "journalArticle",
            "title": "Timber transported to Antarctica: a potential and undesirable carrier for alien fungi and insects",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Piotr",
                    "lastName": "Osyczka"
                },
                {
                    "creatorType": "author",
                    "firstName": "Piotr",
                    "lastName": "Mleczko"
                },
                {
                    "creatorType": "author",
                    "firstName": "Dariusz",
