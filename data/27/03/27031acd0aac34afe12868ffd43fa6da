            "key": "4PBJENE2",
            "version": 84012,
            "itemType": "journalArticle",
            "title": "Non-indigenous species as stressors in estuarine and marine communities: Assessing invasion impacts and interactions",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Gregory M.",
                    "lastName": "Ruiz"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paul>",
                    "lastName": "Fofonoff"
                },
                {
                    "creatorType": "author",
                    "firstName": "Anson H.",
                    "lastName": "Hines"
