            "version": 83789,
            "itemType": "journalArticle",
            "title": "Origin of the amphibian chytrid fungus",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ché",
                    "lastName": "Weldon"
                },
                {
                    "creatorType": "author",
