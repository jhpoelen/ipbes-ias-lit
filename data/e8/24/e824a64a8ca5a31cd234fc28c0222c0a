            "version": 83972,
            "itemType": "journalArticle",
            "title": "Towards an integrated animal health surveillance system in Tanzania: making better use of existing and potential data sources for early warning surveillance",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Janeth",
                    "lastName": "George"
                },
                {
                    "creatorType": "author",
                    "firstName": "Barbara",
                    "lastName": "Häsler"
                },
                {
                    "creatorType": "author",
                    "firstName": "Erick",
                    "lastName": "Komba"
                },
                {
                    "creatorType": "author",
                    "firstName": "Calvin",
                    "lastName": "Sindato"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mark",
                    "lastName": "Rweyemamu"
