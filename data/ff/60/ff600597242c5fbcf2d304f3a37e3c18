            "key": "3NAT9G8T",
            "version": 87410,
            "itemType": "journalArticle",
            "title": "The ecological biogeography of indigenous and introduced Antarctic springtails",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Helena P",
                    "lastName": "Baird"
                },
                {
                    "creatorType": "author",
                    "firstName": "Charlene",
                    "lastName": "Janion-Scheepers"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mark I",
                    "lastName": "Stevens"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rachel I",
