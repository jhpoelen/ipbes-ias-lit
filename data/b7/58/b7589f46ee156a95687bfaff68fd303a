            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Sílvia R.",
                    "lastName": "Ziller"
                },
                {
                    "creatorType": "author",
                    "firstName": "Michele de Sá",
                    "lastName": "Dechoum"
                },
                {
                    "creatorType": "author",
                    "firstName": "Raphaela Aparecida Duarte",
                    "lastName": "Silveira"
