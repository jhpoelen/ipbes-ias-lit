            "key": "87JKDMUG",
            "version": 87278,
            "itemType": "journalArticle",
            "title": "A hybrid modeling approach to simulating foot-and-mouth disease outbreaks in Australian livestock",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Richard A.",
                    "lastName": "Bradhurst"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sharon E.",
                    "lastName": "Roche"
                },
                {
                    "creatorType": "author",
                    "firstName": "Iain J.",
                    "lastName": "East"
