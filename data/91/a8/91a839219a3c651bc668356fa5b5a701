            "version": 87266,
            "itemType": "journalArticle",
            "title": "The Importance of Stakeholder Engagement in Invasive Species Management: A Cross-jurisdictional Perspective in Ireland",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "K. E.",
                    "lastName": "Stokes"
                },
                {
                    "creatorType": "author",
                    "firstName": "K. P.",
                    "lastName": "O℉Neill"
                },
                {
                    "creatorType": "author",
                    "firstName": "W. I.",
                    "lastName": "Montgomery"
                },
                {
                    "creatorType": "author",
                    "firstName": "J. T. A.",
