            "key": "G6QLGHXL",
            "version": 87401,
            "itemType": "journalArticle",
            "title": "Does governance play a role in the distribution of invasive alien species?",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Thomas",
                    "lastName": "Evans"
                },
                {
                    "creatorType": "author",
                    "firstName": "Philine zu",
                    "lastName": "Ermgassen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Tatsuya",
