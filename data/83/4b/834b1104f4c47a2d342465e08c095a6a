            "key": "7JQE77QH",
            "version": 84060,
            "itemType": "journalArticle",
            "title": "A decision-making framework for restoring riparian zones degraded by invasive alien plants in South Africa",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "P. M.",
                    "lastName": "Holmes"
                },
                {
                    "creatorType": "author",
                    "firstName": "D. M.",
                    "lastName": "Richardson"
                },
                {
                    "creatorType": "author",
                    "firstName": "K. J.",
                    "lastName": "Esler"
                },
                {
                    "creatorType": "author",
                    "firstName": "E. T. F.",
                    "lastName": "Witkowski"
