            "key": "UZLJVLFN",
            "version": 87249,
            "itemType": "journalArticle",
            "title": "Evolutionary responses of native plant species to invasive plants: a review",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ayub M. O.",
                    "lastName": "Oduor"
                }
