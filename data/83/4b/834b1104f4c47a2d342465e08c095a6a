            "version": 87417,
            "itemType": "journalArticle",
            "title": "Regeneration of <i>Metrosideros polymorpha</i> forests in Hawaii after landscape-level canopy dieback",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Linda",
                    "lastName": "Mertelmeyer"
                },
                {
                    "creatorType": "author",
                    "firstName": "James D.",
                    "lastName": "Jacobi"
                },
                {
                    "creatorType": "author",
                    "firstName": "Dieter",
                    "lastName": "Mueller-Dombois"
