            "version": 87287,
            "itemType": "journalArticle",
            "title": "Eradicate, contain, or live with it? Collaborating with stakeholders to evaluate responses to invasive species",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Shuang",
                    "lastName": "Liu"
                },
                {
                    "creatorType": "author",
                    "firstName": "David",
                    "lastName": "Cook"
                }
