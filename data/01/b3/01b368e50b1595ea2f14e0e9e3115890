        "data": {
            "key": "TA9IRPPZ",
            "version": 87374,
            "itemType": "journalArticle",
            "title": "Ecological, behavioral, and genetic factors influencing the recombinant control of invasive pests",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Nicholas J.",
                    "lastName": "Bax"
                },
                {
                    "creatorType": "author",
