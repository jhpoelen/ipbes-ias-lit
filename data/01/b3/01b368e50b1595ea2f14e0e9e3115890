            "itemType": "journalArticle",
            "title": "Selfish genetic elements",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "J. Arvid",
                    "lastName": "Ågren"
                },
                {
                    "creatorType": "author",
                    "firstName": "Andrew G.",
                    "lastName": "Clark"
