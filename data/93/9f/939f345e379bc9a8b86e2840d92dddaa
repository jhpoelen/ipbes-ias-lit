            "version": 87273,
            "itemType": "journalArticle",
            "title": "Transmission of <i>Phytophthora ramorum</i> in mixed-evergreen forest in California",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "J. M.",
                    "lastName": "Davidson"
                },
                {
                    "creatorType": "author",
                    "firstName": "A. C.",
                    "lastName": "Wickland"
                },
                {
                    "creatorType": "author",
                    "firstName": "H. A.",
                    "lastName": "Patterson"
                },
                {
                    "creatorType": "author",
                    "firstName": "K. R.",
