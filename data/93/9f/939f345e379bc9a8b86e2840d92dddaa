            "key": "E3HRU8PL",
            "version": 83979,
            "itemType": "journalArticle",
            "title": "The Historical Ecology of Game Species Introductions in Hawai‘i1",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Deidre J.",
                    "lastName": "Duffy"
                },
