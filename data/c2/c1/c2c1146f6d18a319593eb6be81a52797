            "version": 87209,
            "itemType": "journalArticle",
            "title": "Assessment of chemical treatments for controlling <i>Didemnum vexillum</i>, other biofouling, and predatory sea stars in Pacific oyster aquaculture",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "K. C.",
                    "lastName": "Rolheiser"
                },
                {
                    "creatorType": "author",
                    "firstName": "A.",
                    "lastName": "Dunham"
                },
