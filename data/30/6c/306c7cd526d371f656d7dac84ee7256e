            "version": 84040,
            "itemType": "journalArticle",
            "title": "Economic costs of invasive alien species in Spain",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Elena",
                    "lastName": "Angulo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Liliana",
                    "lastName": "Ballesteros-Mejia"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ana",
                    "lastName": "Novoa"
