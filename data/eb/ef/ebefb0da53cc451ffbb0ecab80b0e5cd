            "itemType": "journalArticle",
            "title": "Pathogens as potential hitchhikers on intercontinental dust",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ilka",
                    "lastName": "Toepfer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jocelyne",
                    "lastName": "Favet"
                },
                {
                    "creatorType": "author",
                    "firstName": "Anette",
                    "lastName": "Schulte"
                },
                {
                    "creatorType": "author",
                    "firstName": "Maria",
                    "lastName": "Schmölling"
                },
                {
                    "creatorType": "author",
                    "firstName": "Werner",
                    "lastName": "Butte"
                },
                {
                    "creatorType": "author",
                    "firstName": "Eric W",
                    "lastName": "Triplett"
