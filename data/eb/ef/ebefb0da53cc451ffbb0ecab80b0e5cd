            "version": 83829,
            "itemType": "journalArticle",
            "title": "Introduced annual grass increases regional fire activity across the arid western USA (1980-2009)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Jennifer K.",
                    "lastName": "Balch"
                },
                {
                    "creatorType": "author",
                    "firstName": "Bethany A.",
                    "lastName": "Bradley"
                },
                {
                    "creatorType": "author",
                    "firstName": "Carla M.",
