            "version": 83829,
            "itemType": "journalArticle",
            "title": "Hurricanes accelerated the Florida-Bahamas lionfish invasion",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "MW",
                    "lastName": "Johnston"
                },
                {
                    "creatorType": "author",
                    "firstName": "SJ",
