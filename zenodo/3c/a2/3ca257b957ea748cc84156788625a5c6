nia.", "access_right": "restricted", "creators": [{"name": "Tildesley, Michael J.", "affiliation": null}, {"name": "Smith, Gary", "affiliation": null}, {"name": "Keeling, Matt J.", "affiliation": null}], "keywords": ["Chapter 5", "biodiversity", "environment assessment", "IPBES", "Alien Invasive Species Assessment AIS", "invasive species"], "related_identifiers": [{"identifier": "hash://md5/ab3f4e68ef3e9ceaa6a391551f329d77", "relation": "hasVersion", "scheme": "url"}, {"identifier": "hash://sha256/37362fe07ba28fd74410dec4eca16b39aea21a4ccf8eeeeceaf864d48f067ea9", "relation": "hasVersion", "scheme": "url"}, {"identifier": "zotero://select/groups/2352922/items/HJXQPPW3", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "https://zotero.org/groups/2352922/items/HJXQPPW3", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "https://linker.bio/cut:hash://md5/9b1f93e1c8c6c135ba92fdbccb63a5ed!/b116675-120909", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "hash://md5/359bd494433933b868f85e88d3437f76", "relation": "isPartOf", "scheme": "url"}, {"identifier": "10.1016/j.prevetmed.2011.11.007", "relation": "isVariantFormOf", "resource_type": "publication", "scheme": "doi"}, {"identifier": "10.5281/zenodo.7430682", "relation": "isCitedBy", "resource_type": "publication-report", "scheme": "doi"}, {"identifier": "10.5281/zenodo.1410543", "relation": "isCompiledBy", "resource_type": "software", "scheme": "doi"}], "resource_type": {"title": "Journal article", "type": "publication", "subtype": "article"}, "journal": {"issue": "3", "pages": "224-239", "title": "Preventive Veterinary Medicine", "volume": "104"}, "alternate_identifiers": [{"identifier": "hash://md5/ab3f4e68ef3e9ceaa6a391551f329d77"}, {"identifier": "urn:lsid:zotero.org:groups:2352922:items:HJXQPPW3"}, {"identifier": "10.1016/j.prevetmed.2011.11.007"}], "communities": [{"id": "biosyslit"}, {"id": "ipbes-ias"}], "relations": {"version": [{"index": 0, "is_last": true, "parent": {"pi